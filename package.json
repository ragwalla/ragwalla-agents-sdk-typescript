{
  "name": "@ragwalla/agents-sdk",
  "version": "1.1.0",
  "description": "TypeScript SDK for Ragwalla Agents API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./workers": {
      "import": "./dist/workers.js",
      "require": "./dist/workers.js",
      "types": "./dist/workers.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:node && npm run build:workers",
    "build:node": "tsc",
    "build:workers": "tsc --project tsconfig.workers.json --outDir dist",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build",
    "test-client": "ts-node test-client.ts",
    "test-client:debug": "DEBUG=true ts-node test-client.ts --debug",
    "test-client:basic": "SCENARIOS=agent-management,token-generation,basic-chat ts-node test-client.ts",
    "test-client:advanced": "SCENARIOS=long-response,multi-message,connection-recovery,timeout-handling ts-node test-client.ts",
    "test-client:quick": "SCENARIOS=basic-chat TIMEOUT=10000 ts-node test-client.ts"
  },
  "keywords": [
    "ragwalla",
    "agents",
    "ai",
    "sdk",
    "typescript"
  ],
  "author": "Ragwalla",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "ws": "^8.14.2"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "ws": "^8.14.2"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ragwalla/ragwalla-agents-sdk-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/ragwalla/ragwalla-agents-sdk-typescript/issues"
  },
  "homepage": "https://github.com/ragwalla/ragwalla-agents-sdk-typescript#readme"
}
